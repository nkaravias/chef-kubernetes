apiVersion: v1
clusters:
- cluster:
    certificate-authority-data: <%=@ca_certificate_base64 %>
    <%@servers.each do |server| %>
    server: <%=server %>
    <% end %>
  name: <%=@cluster_name%>
contexts:
- context:
    cluster: <%=@cluster_name%>
    user: kube-proxy
  name: default
current-context: default
kind: Config
preferences: {}
users:
- name: kube-proxy
  user:
    client-certificate-data: <%=@kube_proxy_client_certificate_base64 %>
    client-key-data: <%=@kube_proxy_client_certificate_key_base64 %>
